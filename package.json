{
  "name": "LeaseHawkChatBot",
  "version": "0.1.0",
  "description": "React / Redux / Typescript / Webpack - Starter",
  "main": "dist/app.js",
  "license": "ISC",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack-dashboard -- webpack --config configs/webpack.config.js --env.build --env.sourceMap --env.isDev",
    "publish": "yarn run clean && webpack --config configs/webpack.config.js --env.build",
    "dev": "webpack-dev-server --config configs/webpack.config.js --open --env.isDev",
    "dev:dashboard": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.isDev",
    "dev:awesome": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.awesome --env.isDev",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "reinstall": "rm -rf node_modules && yarn",
    "reinstall:win": "rd /s /q node_modules && yarn",
    "precommit": "yarn run lint",
    "prepush": "yarn run lint & yarn run tsc"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "tslib": "^1.8.1",
    "typesafe-actions": "^1.1.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@svgr/webpack": "^2.1.1",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "3",
    "@types/react-redux": "^7.0.1",
    "@types/redux-thunk": "^2.1.0",
    "@types/webpack": "^3.8.2",
    "@types/webpack-dev-server": "^2.9.2",
    "@types/webpack-env": "^1.13.3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "react-hot-loader": "3",
    "rimraf": "^2.6.2",
    "smooth-scroll-into-view-if-needed": "^1.1.23",
    "styled-components": "^4.3.2",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-react": "^3.4.0",
    "typescript": "^2.8.4",
    "url-loader": "^1.0.1",
    "webpack": "3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.11.0",
    "yarn": "^1.6.0",
    "yup": "^0.26.6"
  }
}
