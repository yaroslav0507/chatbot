{
  "name": "LeaseHawkChatBot",
  "version": "0.1.0",
  "description": "React / Redux / Typescript / Webpack - Starter",
  "main": "dist/app.js",
  "license": "ISC",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack-dashboard -- webpack --config configs/webpack.config.js --env.build --env.sourceMap --env.isDev",
    "publish": "yarn run clean && webpack --config configs/webpack.config.js --env.build",
    "dev": "webpack-dev-server --config configs/webpack.config.js --open --env.isDev",
    "dev:dashboard": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.isDev",
    "dev:awesome": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.awesome --env.isDev",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:watch": "jest --config jest.json --watch",
    "test:update": "jest --config jest.json -u",
    "test:coverage": "jest --config jest.json --coverage",
    "reinstall": "rm -rf node_modules && yarn",
    "reinstall:win": "rd /s /q node_modules && yarn",
    "precommit": "yarn run lint",
    "prepush": "yarn run lint & yarn run tsc & yarn run test",
    "deploy": "echo 'deploy not set!'",
    "mock": "node mock/app.js"
  },
  "dependencies": {
    "@date-io/moment": "^1.1.0",
    "@types/classnames": "^2.2.5",
    "@types/moment-timezone": "^0.5.10",
    "@types/recompose": "^0.27.0",
    "@types/reselect": "^2.2.0",
    "@types/throttle-debounce": "^1.1.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "qs": "^6.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-modal": "^3.4.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "tslib": "^1.8.1",
    "typesafe-actions": "^1.1.2",
    "url-search-params-polyfill": "^4.0.0",
    "utility-types": "^1.0.0-rc.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@date-io/date-fns": "^1.0.1",
    "@svgr/webpack": "^2.1.1",
    "@types/enzyme": "^3.1.6",
    "@types/jest": "^22.0.1",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "3",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.11",
    "@types/redux-thunk": "^2.1.0",
    "@types/webpack": "^3.8.2",
    "@types/webpack-dev-server": "^2.9.2",
    "@types/webpack-env": "^1.13.3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "formdata-polyfill": "^3.0.13",
    "formik": "^1.4.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "import-glob-loader": "^1.1.0",
    "jest": "^22.1.1",
    "node-sass": "^4.11.0",
    "postcss-loader": "^2.1.3",
    "react-hot-loader": "3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "smooth-scroll-into-view-if-needed": "^1.1.23",
    "style-loader": "^0.20.3",
    "styled-components": "^4.3.2",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-react": "^3.4.0",
    "typescript": "^2.8.4",
    "url-loader": "^1.0.1",
    "webpack": "3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dashboard": "^0.4.0",
